{
  "name": "@pagegl/builder-core",
  "version": "0.1.0",
  "description": "Website builder core schema and export",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
